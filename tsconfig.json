{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noUncheckedIndexedAccess": true,
    "noImplicitOverride": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    }
  },
  "include": [
    "./src/**/*.ts",
    "./src/**/*.tsx",
    "./.expo/types/**/*.ts",
    "./expo-env.d.ts",
    "./nativewind-env.d.ts",
    "./tailwind.config.ts"
  ]
}
